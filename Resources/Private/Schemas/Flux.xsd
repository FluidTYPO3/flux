      <xsd:attribute type="xsd:boolean" name="hideParent" default="false">
        <xsd:annotation>
          <xsd:documentation><![CDATA[If TRUE, hides the parent field]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:anySimpleType" name="variables" default="array (&#10;)">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Freestyle variables which become assigned to the resulting Component - can then be read from that Component outside this Fluid template and in other templates using the Form object from this template]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="extensionName" default="NULL">
        <xsd:annotation>
          <xsd:documentation><![CDATA[If provided, enables overriding the extension context for this and all child nodes. The extension name is otherwise automatically detected from rendering context.]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="table" default="NULL">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Table to search. If left out will use the table defined by the parent field]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="pidList" default="'0'">
        <xsd:annotation>
          <xsd:documentation><![CDATA[List of storage page UIDs]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:integer" name="pidDepth" default="99">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Depth of recursive storage page UID lookups]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:integer" name="minimumCharacters" default="1">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Minimum number of characters that must be typed before search begins]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:integer" name="maxPathTitleLength" default="15">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Maximum path segment length - crops titles over this length]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:boolean" name="searchWholePhrase" default="false">
        <xsd:annotation>
          <xsd:documentation><![CDATA[A match requires a full word that matches the search value]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="searchCondition" default="''">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Search condition - for example, if table is pages "doktype = 1" to only allow standard pages]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="cssClass" default="''">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Use this CSS class for all list items]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="receiverClass" default="''">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Class reference, target class should be derived from "t3lib_tceforms_suggest_defaultreceiver"]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute type="xsd:string" name="renderFunc" default="''">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Reference to function which processes all records displayed in results]]></xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
