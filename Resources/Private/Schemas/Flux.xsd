      <xsd:attribute type="xsd:string" name="condition" default="NULL">
        <xsd:annotation>
          <xsd:documentation><![CDATA[Condition to use when selecting from "foreignTable", supports FlexForm `foreign_table_where` markers]]></xsd:documentation>
        </xsd:annotation>
